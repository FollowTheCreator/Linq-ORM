@model List<UserIdEmailName>

    <div>
        <table class="table mt-3">
            <thead class="thead-light">
                <tr>
                    <th class="text-dark">
                        Id
                    </th>
                    <th class="text-dark">
                        Name
                    </th>
                    <th class="text-dark">
                        Email
                    </th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>@item.Id</td>
                        <td>@item.Name</td>
                        <td>@item.Email</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>